{
  "manifest_version": 2,
 
  "name": "Hide Social Count",
  "version": "0.4",
  "description": "Hide numbers from twitter, facebook, linkedin and instagram",
 
  "content_scripts": [{
    "css": ["styles.css"],
    "matches": ["https://twitter.com/*"]
  },
  {
    "css": ["styles.css"],
    "matches": ["https://www.facebook.com/*"]
  },
  {
    "css": ["styles.css"],
    "matches": ["https://www.instagram.com/*"]
  },
  {
    "css": ["styles.css"],
    "matches": ["https://www.linkedin.com/feed/*"]
  }]
}